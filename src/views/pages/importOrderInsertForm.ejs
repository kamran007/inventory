<%- include('../partials/head.ejs') %>     
            <div class="mdk-drawer-layout__content mdk-header-layout__content--scrollable">
                <!-- CONTENT BODY -->
                <div class="container-fluid ui-grid">
                    <div class="card">
                        <div class="card-header text-center">
                            <h3>Import Order Form</h3>
                        </div>
                        <div class="card-body">
                            <form action="/importOrder/insert" method="POST" id="ImportOrderForm">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="row my-3">
                                            <div class="row col-8">
                                                <label for="exporter" class="col-3 col-form-label d-block text-end">Customer Name</label>
                                                <div class="col-9">
                                                        <select class="select-2 " id="exporter" name="exporter">
                                                            <option selected>Select One Customer</option>
                                                            <% for (let exporter of exporters){ %>
                                                            <option value="<%= exporter._id %>"> <%= exporter.name %> </option>
                                                            <% } %>
                                                        </select>
                                                </div>
                                            </div>
                                            <div class="row col-4">
                                                <label for="date" class="col-4 d-block text-end">Date </label>
                                                <div class="col-8"><input type="date" name="date" id="date" class="form-control" value=""></div>
                                            </div>
                                        </div>
                                        <div class="row my-3">
                                            <div class="row col-4">
                                                <label for="product" class="col-6 col-form-label d-block text-end">Product Name</label>
                                                <div class="col-6">
                                                    <select name="product" id="product" class="select-2">
                                                        <option value="" selected>Select a product </option>
                                                        <% for (let product of products){ %>
                                                        <option value="<%= product._id %>" > <%= product.productName %> </option>
                                                        <% } %>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="row col-4">
                                                <label for="quantity" class="col-6 col-form-label d-block text-end">Quantity</label>
                                                <div class="col-6">
                                                    <input type="number" id="quantity" name="quantity" class="form-control">
                                                </div>
                                            </div>
                                            <div class="row col-4">
                                                <label for="totalPrice" class="col-6 col-form-label d-block text-end">Total Price</label>
                                                <div class="col-6">
                                                    <input type="number" id="totalPrice" name="totalPrice" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row my-3 justify-content-center">
                                    <div class="card">
                                        <div class="card-header">
                                            <h4>Cost Insert Form</h4>
                                        </div>
                                        <div class="card-body row">
                                            <div class="row col-5">
                                                <label for="costDescription" class="col-6 col-form-label d-block text-end">Cost Description</label>
                                                <div class="col-6">
                                                    <input type="text" id="costDescription" name="costDescription" class="form-control">
                                                </div>
                                            </div>
                                            <div class="row col-5">
                                                <label for="amount" class="col-6 col-form-label d-block text-end">Amount</label>
                                                <div class="col-6">
                                                    <input type="number" id="amount" name="amount" class="form-control">
                                                </div>
                                            </div>
                                            <div class="row col-2">
                                                <div class="col"><button id="costAdd" class="btn btn-primary"
                                                    form="form1">Add</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-container">
                                    <div class="card my-2">
                                        <div class="card-header text-center fw-4 fs-4">Cost Table</div>
                                        <div class="card-body scroll">
                                            <table class="table" id="CostTable" style="table-layout: fixed;">
                                                <thead>
                                                    <tr>
                                                        <th scope="col">Cost Description</th>
                                                        <th scope="col">Amount</th>
                                                        <th scope="col">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div class="invisible">
                                    <input type="text" name="c_d[]" id="c_d" />
                                    <input type="text" name="c_a[]" id="c_a" />
                                </div>
                                <div class="row my-2">
                                    <div class="col-5"></div>
                                    <div class="col my-2">
                                        <label for="total" class="d-block text-end">Total</label>
                                    </div>
                                    <div class="col">
                                        <input type="number" name="total" id="total" value="0" class="form-control"
                                            readonly>
                                    </div>
                                    <div class="col"></div>
                                </div>
                                <div class="row my-2">
                                    <div class="col-5"></div>
                                    <div class="col my-2">
                                        <label for="totalCost" class="d-block text-end">Total Cost</label>
                                    </div>
                                    <div class="col">
                                        <input type="number" name="totalCost" id="totalCost" value="0" class="form-control"
                                            readonly>
                                    </div>
                                    <div class="col"></div>
                                </div>
                                <div class="row my-2">
                                    <div class="col-10"></div>
                                    <div class="col d-block text-end"><input type="button" id="resetImport" value="Reset" class="btn btn-danger">
                                    </div>
                                    <div class="col d-block text-end"><input type="submit" form="ImportOrderForm" value="Insert" class="btn btn-primary">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <%- include('../partials/alert.ejs') %>

            <!-- drawer -->
            <%- include('../partials/sidebar.ejs') %> 

        </div>
        <!-- // END drawer-layout -->
    </div>
    <%- include('../partials/tail.ejs') %> 
    <script src="/assets/js/buy.js"></script>
    
   <%- include('../partials/footer.ejs') %>
